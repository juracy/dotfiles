// Place your settings in this file to overwrite the default settings
{
	// Core
	"breadcrumbs.enabled": true,
	"diffEditor.ignoreTrimWhitespace": true,
	"diffEditor.renderSideBySide": true,
	"editor.fontFamily": "Iosevka, Victor Mono, Operator Mono, Noto Color Emoji, JetBrains Mono",
	"editor.fontLigatures": true,
	"editor.fontSize": 20,
	"editor.fontWeight": "200",
	"editor.letterSpacing": 0.25,
	"editor.lineHeight": 24,
	"editor.mouseWheelZoom": true,
	"editor.renderControlCharacters": false,
	"editor.renderWhitespace": "selection",
	"editor.semanticHighlighting.enabled": true,
	"editor.snippetSuggestions": "top",
	"editor.suggestSelection": "first",
	"editor.tabCompletion": "on",
	"editor.tabSize": 4,
	"editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?‚ü®‚ü©‚Äú‚Äù",
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"extensions.autoUpdate": false,
	"extensions.ignoreRecommendations": false,
	"files.trimTrailingWhitespace": true,
	"files.insertFinalNewline": true,
	"files.associations": {
		"*.jinja": "html",
		"*.html": "html",
		".pylintrc": "ini",
		".env": "ini"
	},
	"files.exclude": {
		"**/node_modules": true,
		"**/.git": true,
		"**/.venv": true,
		"**/typings": true,
		"**/.DS_Store": true,
		"**/target": true,
		"**/build": true,
		"**/dist": true,
		"**/__pycache__": true,
		"**/*.cache": true,
		"**/diag-????????????????????????????????.*": true,
		"**/.sass-cache": true,
		"**/staticfiles": true,
		"**/CVS": true,
		"**/*.js": { "when": "$(basename).ts"},
		"**/*.js.map": true
	},
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.untrackedChanges": "separate",
	"http.proxyStrictSSL": false,
	"javascript.implicitProjectConfig.experimentalDecorators": true,
	"search.exclude": {
		"**/node_modules": true,
		"**/yarn-error.log": true,
		"**/yarn.lock": true,
		"**/coverage/**/*.html": true,
		"**/bower_components": true,
		"**/typings": true,
		"**/.git": true,
		"**/.venv": true,
		"**/target": true,
		"**/build": true,
		"**/dist": true,
		"**/__pycache__": true,
		"**/*.cache": true,
		"**/diag-????????????????????????????????.*": true,
		"**/.sass-cache": true,
		"**/staticfiles": true,
		"**/*.min.*": true,
		"**/CVS": true,
		"**/*.js": { "when": "$(basename).ts"},
		"**/*.js.map": true
	},
	"terminal.integrated.shell.linux": "/bin/sh",
	"terminal.integrated.shellArgs.linux": ["-c", "[ -e /bin/zsh ] && /bin/zsh || ([ -e /bin/bash ] && /bin/bash || /bin/sh)"],
	"terminal.integrated.automationShell.linux": "/bin/bash",
	"terminal.integrated.rendererType": "experimentalWebgl",
	"window.titleSeparator": " üüÑ ",
	"window.zoomLevel": 2,
	"workbench.colorTheme": "Community Material Theme Darker High Contrast",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.colorCustomizations": {
		"activityBarBadge.background": "#FF4081",
		"list.activeSelectionForeground": "#FF4081",
		"list.inactiveSelectionForeground": "#FF4081",
		"list.highlightForeground": "#FF4081",
		"scrollbarSlider.activeBackground": "#FF408150",
		"editorSuggestWidget.highlightForeground": "#FF4081",
		"textLink.foreground": "#FF4081",
		"progressBar.background": "#FF4081",
		"pickerGroup.foreground": "#FF4081",
		"tab.activeBorder": "#FF4081",
		"notificationLink.foreground": "#FF4081",
		"editor.findWidgetResizeBorder": "#FF4081",
		"editorWidget.border": "#FF4081",
		"editorWidget.resizeBorder": "#FF4081",
		"settings.modifiedItemIndicator": "#FF4081",
		"settings.headerForeground": "#FF4081",
		"panelTitle.activeBorder": "#FF4081",
		"breadcrumb.activeSelectionForeground": "#FF4081",
		"menu.selectionForeground": "#FF4081",
		"menubar.selectionForeground": "#FF4081",
		"editor.findMatchBorder": "#FF4081",
		"selection.background": "#FF408140"
	},

	// Plugins
	"TodoParser": {
		"exclude": ["cpp", "c"],
		"folderExclude": ["node_modules", ".vscode", "bower_components", "dist", "CVS", ".git", "coverage"],
		"markers": ["TODO:"],
		"autoDefaultMarkers": true
	},
	"vscodeWorkspaceSwitcher.paths": [
		"/home/juracy/workspace/vscode-workspaces"
	],
	"gitlens.keymap": "chorded",
	"gitlens.advanced.messages": {
		"suppressCommitHasNoPreviousCommitWarning": false,
		"suppressCommitNotFoundWarning": false,
		"suppressFileNotUnderSourceControlWarning": false,
		"suppressGitVersionWarning": false,
		"suppressLineUncommittedWarning": false,
		"suppressNoRepositoryWarning": false,
		"suppressResultsExplorerNotice": false,
		"suppressShowKeyBindingsNotice": true
	},
	"gitlens.views.fileHistory.enabled": true,
	"gitlens.views.lineHistory.enabled": true,
	"gitlens.hovers.currentLine.over": "line",
	"emojisense.languages": {
		"asciidoc": true,
		"markdown": true,
		"git-commit": true,
		"toml": true,
		"html": true,
		"plaintext": {
			"markupCompletionsEnabled": false,
			"emojiDecoratorsEnabled": false
		},
		"python": true
	},
	"cSpell.language": "pt-BR,en,pt",
	"cSpell.userWords": [
